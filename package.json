{

	"scripts": {

		"dev": "npm-run-all --parallel flask tw-watch js-watch ",

		"flask": "python run.py",

		"build": "npm-run-all --sequential tw-build js-build",

		"js-build": "webpack",

		"js-watch": "webpack --watch",

		"tw-build": "tailwindcss -i opendating/static/css/input.css -o opendating/static/dist/css/output.css",

		"tw-watch": "tailwindcss -i opendating/static/css/input.css -o opendating/static/dist/css/output.css"

	},

	"devDependencies": {

		"npm-run-all": "^4.1.5",

		"tailwindcss": "^3.4.14",

		"webpack": "^5.96.1",

		"webpack-cli": "^5.1.4"

	},

	"dependencies": {

		"alpinejs": "^3.14.3",

		"feather-icons": "^4.29.2",

		"socket.io-client": "^4.8.1"

	}

}

